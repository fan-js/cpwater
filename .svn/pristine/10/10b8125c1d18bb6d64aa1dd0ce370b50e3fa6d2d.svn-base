package cn.ritac.mmbs.mybatis.model;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.*;

@Table(name = "mmbs_devices_do")
public class DevicesDo implements Serializable {
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY, generator = "SELECT LAST_INSERT_ID()")
    private Integer id;

    @Column(name = "device_id")
    private Integer deviceId;

    private Boolean door;

    private Boolean light;

    private Boolean beep;

    private Boolean fan;

    private Boolean heating;

    @Column(name = "bak_switch")
    private Boolean bakSwitch;

    @Column(name = "camera_pwr")
    private Boolean cameraPwr;

    @Column(name = "fill_light_pwr")
    private Boolean fillLightPwr;

    @Column(name = "onu_pwr")
    private Boolean onuPwr;

    @Column(name = "router_pwr")
    private Boolean routerPwr;

    @Column(name = "update_time")
    private Date updateTime;

    private static final long serialVersionUID = 1L;

    /**
     * @return id
     */
    public Integer getId() {
        return id;
    }

    /**
     * @param id
     */
    public void setId(Integer id) {
        this.id = id;
    }

    /**
     * @return device_id
     */
    public Integer getDeviceId() {
        return deviceId;
    }

    /**
     * @param deviceId
     */
    public void setDeviceId(Integer deviceId) {
        this.deviceId = deviceId;
    }

    /**
     * @return door
     */
    public Boolean getDoor() {
        return door;
    }

    /**
     * @param door
     */
    public void setDoor(Boolean door) {
        this.door = door;
    }

    /**
     * @return light
     */
    public Boolean getLight() {
        return light;
    }

    /**
     * @param light
     */
    public void setLight(Boolean light) {
        this.light = light;
    }

    /**
     * @return beep
     */
    public Boolean getBeep() {
        return beep;
    }

    /**
     * @param beep
     */
    public void setBeep(Boolean beep) {
        this.beep = beep;
    }

    /**
     * @return fan
     */
    public Boolean getFan() {
        return fan;
    }

    /**
     * @param fan
     */
    public void setFan(Boolean fan) {
        this.fan = fan;
    }

    /**
     * @return heating
     */
    public Boolean getHeating() {
        return heating;
    }

    /**
     * @param heating
     */
    public void setHeating(Boolean heating) {
        this.heating = heating;
    }

    /**
     * @return bak_switch
     */
    public Boolean getBakSwitch() {
        return bakSwitch;
    }

    /**
     * @param bakSwitch
     */
    public void setBakSwitch(Boolean bakSwitch) {
        this.bakSwitch = bakSwitch;
    }

    /**
     * @return camera_pwr
     */
    public Boolean getCameraPwr() {
        return cameraPwr;
    }

    /**
     * @param cameraPwr
     */
    public void setCameraPwr(Boolean cameraPwr) {
        this.cameraPwr = cameraPwr;
    }

    /**
     * @return fill_light_pwr
     */
    public Boolean getFillLightPwr() {
        return fillLightPwr;
    }

    /**
     * @param fillLightPwr
     */
    public void setFillLightPwr(Boolean fillLightPwr) {
        this.fillLightPwr = fillLightPwr;
    }

    /**
     * @return onu_pwr
     */
    public Boolean getOnuPwr() {
        return onuPwr;
    }

    /**
     * @param onuPwr
     */
    public void setOnuPwr(Boolean onuPwr) {
        this.onuPwr = onuPwr;
    }

    /**
     * @return router_pwr
     */
    public Boolean getRouterPwr() {
        return routerPwr;
    }

    /**
     * @param routerPwr
     */
    public void setRouterPwr(Boolean routerPwr) {
        this.routerPwr = routerPwr;
    }

    /**
     * @return update_time
     */
    public Date getUpdateTime() {
        return updateTime;
    }

    /**
     * @param updateTime
     */
    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }
}